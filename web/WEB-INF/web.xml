<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>NumberOfPlayers</servlet-name>
        <servlet-class>ru.kfu.itis.NumberOfPlayersServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>NumberOfPlayers</servlet-name>
        <url-pattern>/Number</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Logic</servlet-name>
        <servlet-class>ru.kfu.itis.LogicInitialization</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Logic</servlet-name>
        <url-pattern>/Logic</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Add</servlet-name>
        <servlet-class>ru.kfu.itis.AddServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Add</servlet-name>
        <url-pattern>/Add</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NewGame</servlet-name>
        <servlet-class>ru.kfu.itis.NewGameServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NewGame</servlet-name>
        <url-pattern>/NewGame</url-pattern>
    </servlet-mapping>
</web-app>